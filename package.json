{
  "name": "mernstarter",
  "description": "MERN Starter, Top level await, TypeScript, SWC, Prima",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "build": "swc src -d build",
    "dev": "swc src -d build --watch & nodemon",
    "start": "node --experimental-specifier-resolution=node build/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chatgpt": "^3.3.7",
    "cron": "^2.1.0",
    "express": "^4.18.1",
    "puppeteer": "^19.4.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.249",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.16",
    "dotenv": "^16.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      ".env"
    ],
    "ext": "ts,js,json,.env",
    "exec": "node --experimental-specifier-resolution=node build/server.js"
  }
}
